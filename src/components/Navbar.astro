---
const navmenu  = [
    {
        label: "Partners",
        href: "/#partners",
    }
];

---

<header>
<nav class="relative flex bg-gray-300 dark:bg-zinc-900 p-3">
<div id="brand" class="grid gap-5 content-center">
<a href="/" class="nav-brand"><img src="/logo.png" width="32" height="32"> TTNRT</a>
</div>
<div id="items" class="inline-flex items-center ml-3 gap-3">
{navmenu.map((item, index) => (
  <a class="nav-link" href={item.href}>{item.label}</a>
))}
<div class="group relative">
<span class="nav-link">Pages</span>
<div class="hidden group-hover:block fixed bg-white border rounded p-4 dark:bg-neutral-900 dark:border-neutral-700 dark:shadow-neutral-700/70">
<ul>
<li><a class="nav-link" href="/terms">Terms of use</a></li>
<li><a class="nav-link" href="/privacy">Privacy policy</a></li>
</ul>
</div>
</div>
</div>
</nav>
</header>